
public class NameTest {

	public static void main(String[] args) {
		
		System.out.println("Testing default constructor initialisation and toString method...");
		Name n = new Name();
		System.out.println("Expected both empty strings: Actual: " + n.toString());
		
		System.out.println("Testing set and get methods for first and family name...");
		n.setTitle("Hit 'em Up");
		n.setArtist("Tupac");
		System.out.println("Expected James: Actual: " + n.getTitle());
		System.out.println("Expected Bond: Actual: " + n.getArtist());
		
		
		System.out.println("Testing custom constructor initialisation...");
		Name player1 = new Name("David", "Beckham");
		
		System.out.println("Testing get method for full name...");
		System.out.println("Expected David Beckham: Actual: " + player1.getArtist());
		
	}

}
